{
  "name": "Tiger Assistant",
  "icons": {
    "256": "icon-256.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "script": "background.js"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-256.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "notifications",
    "cookies"
  ]
}
